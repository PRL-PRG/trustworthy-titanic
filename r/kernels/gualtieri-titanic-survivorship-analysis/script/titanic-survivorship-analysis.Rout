> verticalLine <- function(value, name, color) {
+   label <- paste("paste(", shQuote(name), ", ", shQuote("\\n"), ", ", value, ")")
+   geomObj <- list(geom_vline(aes_string(xintercept=value), colour=color, linetype='dashed'),
